{
    "name": "zikula/hook-bundle",
    "type": "symfony-bundle",
    "description": "An bundle consolidating Extension Hook features.",
    "keywords": [ "zikula" ],
    "homepage": "https://ziku.la",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Zikula Team",
            "homepage": "https://ziku.la"
        }
    ],
    "require": {
        "php": ">=7.2.5",
        "symfony/contracts": "5.*",
        "symfony/form": "5.*",
        "symfony/framework-bundle": "5.*",
        "symfony/security": "5.*",
        "symfony/string": "5.*",
        "symfony/validator": "5.*",

        "zikula/core-bundle": "self.version",
        "zikula/coreinstaller-bundle": "self.version",
        "zikula/extensions-module": "self.version",
        "zikula/menu-module": "self.version",
        "zikula/permissions-module": "self.version",
        "zikula/theme-module": "self.version"
    },
    "autoload": {
        "psr-4": { "Zikula\\Bundle\\HookBundle\\": "" }
    }
}
